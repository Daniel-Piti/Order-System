# Environment variables for local development
# Copy this file to .env and fill in your actual values
# Spring Boot will resolve ${VAR_NAME} placeholders from these values

# Database configuration
DB_URL=
DB_USERNAME=
DB_PASSWORD=

# Email configuration (SMTP)
SMTP_HOST=
SMTP_PORT=
SMTP_USERNAME=
SMTP_PASSWORD=
SMTP_FROM_EMAIL=

# JWT secret (min 32 characters)
APP_SECRETS_JWT=

# Admin credentials (BCrypt hashes - optional)
ADMIN_USERNAME_HASH=
ADMIN_PASSWORD_HASH=

# AWS S3 configuration
S3_BUCKET_NAME=
S3_REGION=
S3_PUBLIC_DOMAIN=
